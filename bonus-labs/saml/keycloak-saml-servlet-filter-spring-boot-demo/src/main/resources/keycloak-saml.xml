<keycloak-saml-adapter>
    <SP entityID="http://localhost:8081/saml-servlet-filter/"
        sslPolicy="EXTERNAL"
        logoutPage="/logout.jsp">
        <Keys>
            <Key signing="true">
                <PrivateKeyPem>
                    
                </PrivateKeyPem>
                <CertificatePem>
                    
                </CertificatePem>
            </Key>
        </Keys>
        <IDP entityID="idp"
             signatureAlgorithm="RSA_SHA256">
            <SingleSignOnService signRequest="true"
                                 validateResponseSignature="true"
                                 validateAssertionSignature="false"
                                 requestBinding="POST"
                                 bindingUrl="http://localhost:8080/realms/saml-demo/protocol/saml"/>
            <SingleLogoutService signRequest="true"
                                 signResponse="true"
                                 validateRequestSignature="true"
                                 validateResponseSignature="true"
                                 requestBinding="POST"
                                 responseBinding="POST"
                                 postBindingUrl="http://localhost:8080/realms/saml-demo/protocol/saml"
                                 redirectBindingUrl="http://localhost:8080/realms/saml-demo/protocol/saml"/>
        </IDP>
    </SP>
</keycloak-saml-adapter>
